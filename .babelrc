{
    "plugins": [
      ["module:react-native-dotenv", {
        "envName": "APP_ENV",
        "moduleName": "@env",
        "path": ".env",
        "blocklist": null,
        "allowlist": ["REACT_APP_RAPID_API_KEY"],
        "safe": false,
        "allowUndefined": true,
        "verbose": false
      }]
    ]
}